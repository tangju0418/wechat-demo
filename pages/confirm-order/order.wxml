<!--confirm-order.wxml-->
<view class="my-content">
  <view class="weui-cells weui-cells_after-title weui-cells-right">
    <navigator url="/pages/index/index" class="weui-hd gray-font">账单历史</navigator> 
    <view class="store-content">
      <block wx:for="{{cartItem}}" wx:key="*this">
        <view class="weui-cell" >
          

          <view class="weui-cell__bd">
            <view class="weui-cell-n store-name">{{item.Name}}</view>               <view class="weui-cell-n gray-font">¥{{item.Price}}<text>/份</text></view>
           
          </view>
          <view class="weui-cell__ft weui-cell__ft_in-access"></view>
        </view>
      </block>
    </view>
  </view>  
</view>
